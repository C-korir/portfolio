		<script>
			$(document).ready(function() {
    // Function to handle form submission
    $('form').submit(function(e) {
        e.preventDefault(); // Prevent the default form submission

        // Get form data
        var formData = {
            name: $('#name').val(),
            email: $('#email').val(),
            subject: $('#subject').val(),
            message: $('#comment').val()
        };

        // You can perform additional validation here if needed

        // Send the form data to a server using AJAX
        $.ajax({
            type: 'POST',
            url: 'path/to/your/server-script.php', // Replace with the actual path to your server script
            data: formData,
            success: function(response) {
                // Handle the success response, you can display a success message or redirect the user
                console.log('Form submitted successfully!');
            },
            error: function(error) {
                // Handle the error, you can display an error message
                console.error('Error submitting form:', error);
            }
        });
    });
});

		</script>

